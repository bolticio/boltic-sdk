# Boltic Tables API Configuration
# Copy this file to .env and replace with your actual values

# Your Boltic API Key (required)
BOLTIC_API_KEY=your-api-key-here

# API Base URL (optional - defaults to SIT environment)
BOLTIC_BASE_URL=https://asia-south1.api.fcz0.de/service/panel/boltic-tables

# Environment (optional - defaults to 'sit')
BOLTIC_ENVIRONMENT=sit

# Debug mode (optional - defaults to false)
DEBUG=true

# Timeout in milliseconds (optional - defaults to 30000)
TIMEOUT=30000 